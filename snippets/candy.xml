<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="candy"
           fetch="https://github.com/candyroms-staging"
           revision="refs/heads/14" />

  <remote  name="blaze-gitlab"
           fetch="https://gitlab.com/projectblaze"
           revision="refs/heads/14" />

  <remote  name="blaze-gitea"
